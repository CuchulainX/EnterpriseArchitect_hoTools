    <Search Name="Element usage" Id="{B8CB6915-3567-4a44-AC31-9BEAAD451819}" PkgId="-1" Type="0" LnksToObj="0" CustomSearch="1" AddinAndMethodName="">
      <SrchOn>
        <RootTable Filter="select o.ea_Id As CLASSId, o.Object_type As CLASSTYPE, &#xD;&#xA;'Behaviour / Classifier' As Usage,&#xD;&#xA;o.name As Name,&#xD;&#xA;o.Object_Type As Type,  &#xD;&#xA;o.stereotype As Stereotype&#xD;&#xA;from t_operation op,  t_object o &#xD;&#xA;where &#xD;&#xA;    op.EA_Id = '&lt;Search Term&gt;' AND&#xD;&#xA;     #DB=JET#               op.Behaviour              = o.EA_Id                    #DB=JET#&#xD;&#xA;  #DB=ORACLE# Cast(op.Behaviour As Varchar2(38)) = o.EA_Id     #DB=ORACLE#&#xD;&#xA;  #DB=SQLSVR#               Substring(op.Behaviour,1,38)              = o.EA_Id              #DB=SQLSVR#&#xD;&#xA;&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;/* --------------------------------------------------------Find State Operation from Class operation (do,entry,exit) -----------------------------------*/&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select op.ea_Id , 'Operation' , 'Operation of Class or State', op.name ,  Type, op.stereotype &#xD;&#xA;from  t_operation op &#xD;&#xA;where &#xD;&#xA;    #DB=JET#               op.Behaviour                           = '&lt;Search Term&gt;' #DB=JET#&#xD;&#xA; #DB=ORACLE# Cast(op.Behaviour As Varchar2(38)) = '&lt;Search Term&gt;' #DB=ORACLE#&#xD;&#xA; #DB=SQLSVR#         Substring(op.Behaviour,1,38)   = '&lt;Search Term&gt;' #DB=SQLSVR# &#xD;&#xA;&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;/* --------------------------------------------------------Find Class Operation from State operation (do,entry,exit) -----------------------------------*/&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select op.ea_Id , 'Operation' , 'Operation Class&lt;--&gt; State', op.name ,  op.Type, op.stereotype &#xD;&#xA;from  t_operation opState, t_operation op &#xD;&#xA;where &#xD;&#xA;#DB=JET#            opState.ea_Id = '&lt;Search Term&gt;'         AND #DB=JET#   &#xD;&#xA;#DB=JET#            opState.Behaviour = op.ea_Id                #DB=JET#   &#xD;&#xA;#DB=ORACLE#  Cast(opState.Behaviour as Varchar2(38)) = '&lt;Search term&gt;' AND #DB=ORACLE#             &#xD;&#xA;#DB=ORACLE#  Cast(opState.Behaviour As Varchar2(38)) = op.ea_Id     #DB=ORACLE#&#xD;&#xA;#DB=SQLSVR#            opState.ea_Id = '&lt;Search Term&gt;'         AND #DB=SQLSVR#  &#xD;&#xA;#DB=SQLSVR#            Substring(opState.Behaviour,1,38) = op.ea_Id                #DB=SQLSVR#   &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;/* --------------------------------------------------------Find Call Action -----------------------------------*/&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_Type , 'Call Action', o.name ,  o.Object_Type, o.stereotype &#xD;&#xA;from t_operation op,  t_object o &#xD;&#xA;where &#xD;&#xA; o.Classifier_Id = '&lt;Search Term&gt;'   AND &#xD;&#xA; o.Classifier_Id = op.ea_Id                 &#xD;&#xA;&#xD;&#xA;&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;/* --------------------------------------------------------Find Call from Action of type Call Operation  -----------------------------------*/&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select op.ea_Id , 'Operation' , 'Call Operation', op.name ,  'Operation', op.stereotype &#xD;&#xA;from t_operation op,  t_object o &#xD;&#xA;where &#xD;&#xA;    o.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;AND  o.Classifier_Id = op.ea_Id&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;/* --------------------------------------------------------Find Activity from Action of type Call Behavior  -----------------------------------*/&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_Type , 'Behavior', o.name ,  o.Object_Type, o.stereotype &#xD;&#xA;from t_object o,  t_object o1 &#xD;&#xA;where &#xD;&#xA;    o1.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;AND  o1.Classifier_Id = o.ea_Id&#xD;&#xA;&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;/* --------------------------------------------------------Find Instances from Classifier  -----------------------------------*/&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_Type , 'Object', o.name ,  o.Object_Type, o.stereotype &#xD;&#xA;from t_object o,  t_object o1 &#xD;&#xA;where &#xD;&#xA;    o1.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;AND  o.Classifier_Id = o1.ea_Id&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;/* --------------------------------------------------------Find return type of method-----------------------------------*/&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_Type , 'ReturnType', o.name ,  o.Object_Type, o.stereotype &#xD;&#xA;from t_operation op,  t_object o , t_object o1 &#xD;&#xA;where &#xD;&#xA;    op.EA_Id = '&lt;Search Term&gt;'   AND&#xD;&#xA; #DB=JET#          Format(o.Object_ID)          = op.Classifier   AND  #DB=JET#&#xD;&#xA; #DB=ORACLE#       Cast(o.Object_ID As Varchar2(38)) = op.Classifier   AND  #DB=ORACLE#&#xD;&#xA;    #DB=SQLSVR#          o.Object_ID            = op.Classifier   AND  #DB=SQLSVR#&#xD;&#xA;op.object_id = o1.object_id&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# /*================== Operation Parameter ==============================================*/ #DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select op.ea_Id , 'Operation', &#xD;&#xA;#DB=JET#   'ParameterType ' &amp; opp.Name &amp; ':' &amp; opp.Type,  #DB=JET#&#xD;&#xA;#DB=ORACLE#  Concat(opp.Name,':'),  #DB=ORACLE#&#xD;&#xA;#DB=SQLSVR#   'ParameterType ' + opp.Name + ':' + opp.Type,  #DB=SQLSVR#&#xD;&#xA;o.name ,  o.Object_Type, o.stereotype &#xD;&#xA;&#xD;&#xA;from t_operation op,  t_object o, t_object o1,  t_operationparams opp &#xD;&#xA;where &#xD;&#xA;    o.EA_Id = '&lt;Search Term&gt;'                                              AND&#xD;&#xA; #DB=JET#          Format(o.Object_ID)            = opp.Classifier   AND  #DB=JET#&#xD;&#xA; #DB=ORACLE#       Cast(o.Object_ID As Varchar2(38)) = opp.Classifier   AND  #DB=ORACLE#&#xD;&#xA;    #DB=SQLSVR#          o.Object_ID            = opp.Classifier   AND  #DB=SQLSVR#&#xD;&#xA;     op.OperationID = opp.OperationID                                           AND&#xD;&#xA;     op.object_id = o1.object_id&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# /*================== Operation used in Sequence Diagram ==============================================&#xD;&#xA;*/#DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_type, 'Sequence Message', o.name, o.Object_Type, o.stereotype&#xD;&#xA;&#xD;&#xA;from t_connectortag conTag, t_connector con, t_object o &#xD;&#xA;where &#xD;&#xA;               conTag.Value = '&lt;Search Term&gt;'&#xD;&#xA; AND     conTag.ElementID = con.Connector_ID&#xD;&#xA; AND     con.End_Object_ID = o.Object_ID&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# /*================== Attribute and its type ==============================================&#xD;&#xA;*/#DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id , o.Object_Type, 'Attrubute Type', o.name, o.Object_Type, o.stereotype&#xD;&#xA;&#xD;&#xA;from t_attribute a, t_object o &#xD;&#xA;where &#xD;&#xA;               a.ea_Id = '&lt;Search Term&gt;'          AND&#xD;&#xA; #DB=JET#          Format(o.Object_ID)            = a.Classifier     #DB=JET#&#xD;&#xA; #DB=ORACLE#       Cast(o.Object_ID As Varchar2(38)) = a.Classifier    #DB=ORACLE# &#xD;&#xA;    #DB=SQLSVR#          o.Object_ID            = a.Classifier     #DB=SQLSVR#&#xD;&#xA; &#xD;&#xA;#DB=ORACLE# /*================== Classifier used in Attribute ==============================================&#xD;&#xA;*/#DB=ORACLE#&#xD;&#xA;UNION&#xD;&#xA;select a.ea_Id ,'Attribute', 'Attrubute Type', a.name, 'Attribute', o.stereotype&#xD;&#xA;&#xD;&#xA;from t_attribute a, t_object o &#xD;&#xA;where &#xD;&#xA;               o.ea_Id = '&lt;Search Term&gt;'         AND&#xD;&#xA; #DB=JET#          Format(o.Object_ID)            = a.Classifier     #DB=JET#&#xD;&#xA; #DB=ORACLE#       Cast(o.Object_ID As Number) = a.Classifier    #DB=ORACLE# &#xD;&#xA;    #DB=SQLSVR#          o.Object_ID            = a.Classifier     #DB=SQLSVR#&#xD;&#xA; &#xD;&#xA; &#xD;&#xA;#DB=ORACLE# /*================== Part / Object of class  ==============================================&#xD;&#xA;*/#DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id ,o.Object_type, 'Part',&#xD;&#xA;#DB=JET#                o.name + ':' + o1.name,  #DB=JET#&#xD;&#xA;#DB=ORACLE#   Concat( ':' , o1.name),      #DB=ORACLE#&#xD;&#xA;#DB=SQLSVR#                o.name + ':' + o1.name,  #DB=SQLSVR#&#xD;&#xA;o.object_type, o.stereotype&#xD;&#xA;&#xD;&#xA;from t_object o, t_object o1 &#xD;&#xA;where &#xD;&#xA;   o1.ea_Id = o.PDATA1  AND&#xD;&#xA;  o.PDATA1 = '&lt;Search Term&gt;' &#xD;&#xA; &#xD;&#xA; &#xD;&#xA;#DB=ORACLE# /*================== Class of Part  ==============================================&#xD;&#xA;*/#DB=ORACLE#&#xD;&#xA;UNION&#xD;&#xA;select o.ea_Id ,o.Object_type, 'Part', &#xD;&#xA;#DB=JET#                o.name + ':' + o1.name,  #DB=JET#&#xD;&#xA;#DB=ORACLE#   Concat( ':' , o1.name),      #DB=ORACLE#&#xD;&#xA;#DB=SQLSVR#                o.name + ':' + o1.name,  #DB=SQLSVR#&#xD;&#xA;o.object_type, o.stereotype&#xD;&#xA;&#xD;&#xA;from t_object o, t_object o1 &#xD;&#xA;where &#xD;&#xA;   o1.ea_Id =   '&lt;Search Term&gt;' &#xD;&#xA;   AND o1.PDATA1 = o.ea_Id&#xD;&#xA;&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;/*================== Usage in Diagram  ==============================================&#xD;&#xA;*/&#xD;&#xA;#DB=ORACLE# &#xD;&#xA;UNION&#xD;&#xA;select d.ea_Id, d.Diagram_Type, 'Diagram', d.name,d.Diagram_Type,d.stereotype&#xD;&#xA;from t_diagram d, t_diagramobjects do, t_object o&#xD;&#xA;where    d.diagram_ID = do.diagram_ID&#xD;&#xA;          AND   do.Object_ID = o.Object_ID&#xD;&#xA;          AND    o.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;UNION  &#xD;&#xA;#DB=ORACLE#       /* Trigger for Action */                                                                                     #DB=ORACLE# &#xD;&#xA;    select o.ea_Id , o.Object_type , &#xD;&#xA;'Trigger for Action',&#xD;&#xA;o.name ,&#xD;&#xA;o.Object_Type ,  &#xD;&#xA;o.stereotype &#xD;&#xA;from &#xD;&#xA;     #DB=JET#        (t_xref x INNER JOIN t_object o   on (Mid(x.description,1,38) = o.ea_Id  or Mid(x.description,40,38) = o.ea_Id))                                         #DB=JET#&#xD;&#xA;  #DB=SQLSVR#        (t_xref x INNER JOIN t_object o   on (Substring(x.description,1,38) = o.ea_Id  or Substring(x.description,40,38) = o.ea_Id))                                         #DB=SQLSVR#&#xD;&#xA;  #DB=ORACLE# (t_xref x INNER JOIN t_object o   on (to_char(Substr(Description,1,38)) = o.ea_Id or to_char(Substr(Description,40,38)) = o.ea_Id) )          #DB=ORACLE#&#xD;&#xA;                                             INNER JOIN t_object o1  on (x.client = o1.ea_Id) &#xD;&#xA;where x.behavior = 'trigger'  AND o1.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;&#xD;&#xA;UNION  &#xD;&#xA;#DB=ORACLE#       /* Action for Trigger */                                                                                      #DB=ORACLE# &#xD;&#xA;    select o1.ea_Id , o1.Object_type , &#xD;&#xA;'Action for Trigger' ,&#xD;&#xA;o1.name ,&#xD;&#xA;o1.Object_Type ,  &#xD;&#xA;o1.stereotype &#xD;&#xA;from &#xD;&#xA;     #DB=JET#        (t_xref x INNER JOIN t_object o   on (Mid(x.description,1,38) = o.ea_Id  or Mid(x.description,40,38) = o.ea_Id))                                         #DB=JET#&#xD;&#xA;   #DB=SQLSVR#        (t_xref x INNER JOIN t_object o   on (Substring(x.description,1,38) = o.ea_Id  or Substring(x.description,40,38) = o.ea_Id))                                         #DB=SQLSVR#&#xD;&#xA;  #DB=ORACLE# (t_xref x INNER JOIN t_object o   on (to_char(Substr(Description,1,38)) = o.ea_Id or to_char(Substr(Description,40,38)) = o.ea_Id) )          #DB=ORACLE#&#xD;&#xA;                                             INNER JOIN t_object o1  on (x.client = o1.ea_Id) &#xD;&#xA;where x.behavior = 'trigger'  AND o.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;UNION&#xD;&#xA;#DB=ORACLE#       /* Signal for Trigger */                                                                                      #DB=ORACLE# &#xD;&#xA;    select sig.ea_Id , sig.Object_type , &#xD;&#xA;'Signal for Trigger' ,&#xD;&#xA;sig.name ,&#xD;&#xA;sig.Object_Type ,  &#xD;&#xA;sig.stereotype &#xD;&#xA;from &#xD;&#xA;#DB=JET#            t_xref x   INNER JOIN t_object sig on (Mid(x.description,9,38) = sig.ea_Id)                 #DB=JET#&#xD;&#xA;#DB=SQLSVR#            t_xref x   INNER JOIN t_object sig on (Substring(x.description,9,38) = sig.ea_Id)                 #DB=SQLSVR#&#xD;&#xA;#DB=ORACLE#      t_xref x  INNER JOIN t_object sig on  (to_char(Substr(Description,9,38)) = sig.ea_Id) #DB=ORACLE#&#xD;&#xA;where x.client = '&lt;Search Term&gt;' AND Behavior = 'event'&#xD;&#xA;UNION&#xD;&#xA;#DB=ORACLE#       /* Signal for Trigger */                                                                                      #DB=ORACLE# &#xD;&#xA;    select trig.ea_Id , trig.Object_type , &#xD;&#xA;'Trigger for Signal' ,&#xD;&#xA;trig.name ,&#xD;&#xA;trig.Object_Type ,  &#xD;&#xA;trig.stereotype &#xD;&#xA;from &#xD;&#xA; t_xref x   INNER JOIN t_object trig on (x.client = trig.ea_Id )                                     &#xD;&#xA;where &#xD;&#xA;#DB=JET#            Mid(x.description,9,38) = '&lt;Search Term&gt;'                 #DB=JET#&#xD;&#xA;#DB=SQLSVR#            Substring(x.description,9,38) = '&lt;Search Term&gt;'  #DB=SQLSVR#&#xD;&#xA;#DB=ORACLE#      to_char(Substr(Description,9,38)) = '&lt;Search Term&gt;' #DB=ORACLE#&#xD;&#xA;AND Behavior = 'event'&#xD;&#xA;UNION&#xD;&#xA;#DB=ORACLE# /* Hyperlink auf Diagram */            #DB=ORACLE#&#xD;&#xA;Select lnk.ea_Id , lnk.Object_type , &#xD;&#xA;'Hyperlink to Diagram' ,&#xD;&#xA;lnk.name ,&#xD;&#xA;lnk.Object_Type ,  &#xD;&#xA;lnk.stereotype  &#xD;&#xA;from &#xD;&#xA;t_object lnk LEFT JOIN t_diagram dia on &#xD;&#xA;#DB=JET#         ( lnk.pdata1 = Format(dia.diagram_ID )            ) #DB=JET# &#xD;&#xA;#DB=SQLSVR#         ( lnk.pdata1 = dia.diagram_ID             ) #DB=SQLSVR# &#xD;&#xA;#DB=ORACLE#  ( lnk.pdata1 = Cast(dia.diagram_ID As Varchar2(20))) #DB=ORACLE# &#xD;&#xA;Where dia.ea_Id = '&lt;Search Term&gt;'&#xD;&#xA;&#xD;&#xA;UNION&#xD;&#xA;select trgt.ea_Id, trgt.object_type, 'Trigger', o.name,o.object_Type,o.stereotype&#xD;&#xA;FROM t_xref x, t_connector con, t_object o ,t_object trgt&#xD;&#xA;where x.Behavior = 'trigger'  AND&#xD;&#xA;          x.client = con.ea_Id AND&#xD;&#xA;#DB=SQLSVR#       Substring(x.description,1,38) = o.ea_Id     AND #DB=SQLSVR#&#xD;&#xA;#DB=JET#            Mid(x.description,1,38) = o.ea_Id     AND #DB=JET#&#xD;&#xA;&#x9;&#x9;  con.start_object_id = trgt.object_id AND&#xD;&#xA;&#x9;&#x9;  o.ea_Id  = '&lt;Search Term&gt;'&#xD;&#xA;&#xD;&#xA;Order By 3,4&#xD;&#xA;&#xD;&#xA;#DB=ORACLE#&#xD;&#xA;/*  The search finds the following references for a Method with the Id as a 'Search Term':&#xD;&#xA;- Specified by Aktivity&#xD;&#xA;- Return type&#xD;&#xA;- Parameter Type&#xD;&#xA;- Called by Action &#xD;&#xA;&#xD;&#xA;*/&#xD;&#xA;#DB=ORACLE#" Type="-1">
