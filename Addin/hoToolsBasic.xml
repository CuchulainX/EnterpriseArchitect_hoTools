      <SrchOn>
        <RootTable Filter="select o.ea_Id AS CLASSId, o.Object_Type As CLASSTYPE, o.Name, o.Object_Type, o.stereotype, o.Author,&#xA;#DB=ORACLE# Cast(o.ModifiedDate As Varchar(20)) As Modified, #DB=ORACLE#&#xA;#DB=JET# o.ModifiedDate As Modified, #DB=JET#&#xA;#DB=SQLSVR# o.ModifiedDate As Modified, #DB=SQLSVR#&#xA;&#xA;#DB=ORACLE# Cast(SubStr(o.note,1,200) As Varchar(200)) As Notes       #DB=ORACLE#&#xA;#DB=JET#       Mid(o.note,1,200) As Notes     #DB=JET#&#xA;#DB=SQLSVR#       Substring(o.note,1,200) As Notes     #DB=SQLSVR#&#xA;&#xA;from t_object o left Join t_objectproperties op on (o.Object_ID = op.Object_ID AND Property in ('MKS_id', 'MKS_suspect'))&#xA;where o.ea_Id = '&lt;Search Term&gt;' &#xA;&#xA;&#xA;UNION&#xA;&#xA;select d.ea_Id,  d.Diagram_Type , d.Name, d.Diagram_Type, d.stereotype, d.Author, &#xA;#DB=ORACLE# Cast(d.ModifiedDate As Varchar(20)) As Modified, #DB=ORACLE#&#xA;#DB=JET# d.ModifiedDate As Modified, #DB=JET#&#xA;#DB=SQLSVR# d.ModifiedDate As Modified, #DB=SQLSVR#&#xA;&#xA;#DB=ORACLE# Cast(SubStr(d.notes,1,200) As Varchar(200))        #DB=ORACLE#&#xA;#DB=JET#       Mid(d.notes,1,200)      #DB=JET#&#xA;#DB=SQLSVR#       Substring(d.notes,1,200) As Notes     #DB=SQLSVR#&#xA;from t_diagram  d&#xA;where d.ea_Id = '&lt;Search Term&gt;'&#xA;&#xA;UNION&#xA;select a.ea_Id,  'Attribute' , a.Name, a.type, a.stereotype, ' ', ' ',&#xA;#DB=ORACLE# Cast(SubStr(a.notes,1,200) As Varchar(200))        #DB=ORACLE#&#xA;#DB=JET#       Mid(a.notes,1,200)      #DB=JET#&#xA;#DB=SQLSVR#       Substring(a.notes,1,200) As Notes     #DB=SQLSVR#&#xA;&#xA;from t_attribute  a&#xA;where a.ea_Id = '&lt;Search Term&gt;'&#xA;&#xA;UNION&#xA;select op.ea_Id,  'Operation' , op.Name, 'Operation', op.stereotype, ' ',' ',&#xA;#DB=ORACLE# Cast(SubStr(op.notes,1,200) As Varchar(200))        #DB=ORACLE#&#xA;#DB=JET#       Mid(op.notes,1,200)      #DB=JET#&#xA;#DB=SQLSVR#       Substring(op.notes,1,200) As Notes     #DB=SQLSVR#&#xA;from t_operation  op&#xA;where op.ea_Id = '&lt;Search Term&gt;'&#xA;&#xA;#DB=ORACLE# /* Connector      */  #DB=ORACLE#&#xA;UNION &#xA;select c.ea_Id,  c.connector_Type , c.Name, 'Connector', c.stereotype, d.name,d.ea_Id,&#xA;#DB=ORACLE# Cast(SubStr(c.notes,1,200) As Varchar(200))        #DB=ORACLE#&#xA;#DB=JET#       Mid(c.notes,1,200)      #DB=JET#&#xA;#DB=SQLSVR#       Substring(c.notes,1,200) As Notes     #DB=SQLSVR#&#xA;from (t_connector  c LEFT JOIN t_diagramlinks dl on c.connector_id = dl.connectorid)&#xA;                                 LEFT JOIN t_diagram d on d.Diagram_id = dl.diagramid&#xA;where c.ea_Id = '&lt;Search Term&gt;'&#xA;  &#xA;  &#xA;  #DB=ORACLE# /* Operation Parameter      */  #DB=ORACLE#&#xA;UNION &#xA;select opp.ea_Id,  'Parameter' , opp.Name, 'Parameter', '', opp.name,opp.ea_Id,&#xA;#DB=ORACLE# Cast(SubStr(opp.notes,1,200) As Varchar(200))        #DB=ORACLE#&#xA;#DB=JET#       Mid(opp.notes,1,200)      #DB=JET#&#xA;#DB=SQLSVR#       Substring(opp.notes,1,200) As Notes     #DB=SQLSVR#&#xA;from t_operationparams  opp&#xA;where opp.ea_Id = '&lt;Search Term&gt;'" Type="-1">
